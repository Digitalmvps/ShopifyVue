{
  "name": "vue-typescript-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "debug": "node --inspect-brk server/index.js",
    "serve:dev": "npm run build && cross-env NODE_ENV=development nodemon server/index.js --watch ./server",
    "prepare": "husky install",
    "preserve": "npm run build",
    "serve": "npm run build && cross-env NODE_ENV=production node server/index.js",
    "start": "npm run serve",
    "test": "vitest --reporter=verbose"
  },
  "dependencies": {
    "@originjs/vite-plugin-require-context": "^1.0.9",
    "@shopify/app-bridge-utils": "^3.1.2",
    "@shopify/shopify-api": "^3.0.0",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "sass": "^1.53.0",
    "serve-static": "^1.14.1",
    "vite-plugin-dynamic-import": "^0.1.1",
    "vue": "^3.2.37",
    "vue-class-component": "^8.0.0-0",
    "vue-meta": "^3.0.0-alpha.7",
    "vue-router": "^4.0.0-0",
    "vue3-runtime-template": "^1.0.1",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.0",
    "autoprefixer": "^10.4.7",
    "husky": "^7.0.4",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.14",
    "supertest": "^6.2.3",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vitest": "^0.10.5",
    "vue-tsc": "^0.38.4"
  }
}
